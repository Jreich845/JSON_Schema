      "items": {
        "type": "object",
        "properties": {
          "Company": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "description": "List of company names"
          },
          "CAS Number": {
            "type": "array",
            "items": {
              "type": "string"
            },
